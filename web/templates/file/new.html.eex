<h3>Uploading file to folder "<%= @folder.name %>"</h3>
<%= form_for @changeset, file_path(@conn, :create, @folder), [multipart: true], fn f -> %>
  <%= render Encrypter.LayoutView, "form_error.html", f: f %>

  <div class="form-group">
    <%= label f, :file %>
    <%= file_input f, :file %>
  </div>

  <div class="form-group">
    <%= submit "Upload", class: "btn btn-primary" %>
  </div>
<% end %>
